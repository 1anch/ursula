{
  "client": {
    "name": "{{ inventory_hostname }}",
    "address": "{{ ansible_eth0["ipv4"]["address"] }}",
    "subscriptions": [
      "openstack"
    ]
  },
  "rabbitmq": {
    "host": "{{ sensu.rabbit.host }}",
